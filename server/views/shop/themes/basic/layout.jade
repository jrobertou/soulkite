  // Body
  body
    // Login Modal
    #loginModal.modal.fade(tabindex='-1', role='dialog', aria-hidden='true')
      .modal-dialog
        .modal-content
          .modal-header
            button.close(type='button', data-dismiss='modal', aria-hidden='true')
              i.fa.fa-times
            h2
              | Login or
              a(href='register.html') Register
            p.large Use social accounts
            .social-login
              a.facebook(href='#')
                i.fa.fa-facebook-square
              a.google(href='#')
                i.fa.fa-google-plus-square
              a.twitter(href='#')
                i.fa.fa-twitter-square
          .modal-body
            form.login-form
              .form-group.group
                label(for='log-email') Email
                input#log-email.form-control(type='email', name='log-email', placeholder='Enter your email', required='')
                a.help-link(href='#') Forgot email?
              .form-group.group
                label(for='log-password') Password
                input#log-password.form-control(type='text', name='log-password', placeholder='Enter your password', required='')
                a.help-link(href='#') Forgot password?
              .checkbox
                label
                  input(type='checkbox', name='remember')
                  |  Remember me
              input.btn.btn-black(type='submit', value='Login')
        // /.modal-content
      // /.modal-dialog
    // /.modal
    // Header
    header(data-offset-top='500', data-stuck='600')
      // data-offset-top is when header converts to small variant and data-stuck when it becomes visible. Values in px represent position of scroll from top. Make sure there is at least 100px between those two values for smooth animation
